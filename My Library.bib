@article{Zheng2018,
  title = {Precarity Is a {{Feminist Issue}}: {{Gender}} and {{Contingent Labor}} in the {{Academy}}},
  shorttitle = {Precarity Is a {{Feminist Issue}}},
  author = {Zheng, Robin},
